<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UnlockView">
        <!-- 视图的Tint效果颜色-->
        <attr name="tintColor" format="color" />
        <!-- 视图的背景色-->
        <attr name="backgroundColor" format="color" />
        <!-- 小锁距离视图边界的距离-->
        <attr name="space" format="dimension" />
        <!-- 解锁触发阀值-->
        <attr name="trigger" format="float" />
        <!-- 动画时长 -->
        <attr name="animationTimeDuration" format="integer" />
        <!-- Tint效果动画时长-->
        <attr name="tintAnimationDuration" format="integer" />
        <!-- 是否使用Tint效果-->
        <attr name="tint" format="boolean" />
    </declare-styleable>
</resources>